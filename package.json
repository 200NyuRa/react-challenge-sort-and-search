    "html-webpack-plugin": "^2.28.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.3",
    "node-uuid": "^1.4.7",
    "postcss-loader": "^2.0.5",
    "react-hot-loader": "^3.0.0-beta.4",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "sass-resources-loader": "^1.2.1",
    "style-loader": "^0.17.0",
    "uuid": "^3.0.1",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
