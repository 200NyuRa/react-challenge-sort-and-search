{
  "name": "react-challenge-sort-and-search",
  "version": "1.0.0",
  "description": "Boilerplate for first React Challenge",
  "main": "index.js",
  "scripts": {
    "build": "npm run gh:build",
    "deploy": "npm run gh:deploy",
    "presets": "node scripts/presets",
    "clean": "rimraf public",
    "lint": "npm run lint:eslint-fix",
    "start": "npm run presets && cross-env NODE_ENV=development node scripts/start",
    "lint:eslint": "eslint src --ext .js --ext .jsx",
    "lint:eslint-fix": "eslint src --ext .js --ext .jsx --fix",
    "gh:build": "npm run clean && npm run presets && cross-env NODE_ENV=production BUILD=pages webpack --progress --colors",
    "gh:deploy": "npm run gh:build && gh-pages -d public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rtivital/react-challenge-sort-and-search.git"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Vitaly Rtishchev <rtivital@gmail.com> (http://github.com/rtivital)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rtivital/react-challenge-sort-and-search/issues"
  },
  "homepage": "https://github.com/rtivital/react-challenge-sort-and-search#readme",
  "dependencies": {
    "axios": "^0.14.0",
    "classnames": "^2.2.5",
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "colors": "^1.1.2",
    "cross-env": "^2.0.0",
    "css-loader": "^0.25.0",
    "dreamjs": "^0.1.7",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.9.0",
    "gh-pages": "^0.11.0",
    "glob": "^7.1.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.10.0",
    "node-uuid": "^1.4.7",
    "postcss-loader": "^0.13.0",
    "react-hot-loader": "^3.0.0-beta.4",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "sass-resources-loader": "^1.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.16.1"
  }
}
